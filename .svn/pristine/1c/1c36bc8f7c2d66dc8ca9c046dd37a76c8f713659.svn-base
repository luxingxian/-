			var imgHeight = $(".mainImg").height();
			var headHeight = $(".careHeader").height();
			var footHeight = $(".footer").height();
			var mainHeight = wHeight-headHeight-footHeight;
			var textHeight = mainHeight-imgHeight;
			$(".careMain").outerHeight(mainHeight);
			$(".careMain .text").outerHeight(textHeight);
			
		})
