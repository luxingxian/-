		<script type="text/javascript">
			$(function(){
				 $(".mainSearch").hide();
				$(".searchBlur").hide();
				// $(".searchResult").hide();
				
				//搜索框聚焦效果
				$(".search").keyup(function(){
					if($(this).val().length){
						$(".removeIcon").show();
						$(".navBar").addClass("searchNavBar");
						$(".mainSearch").hide();
						$(".searchResult").show();
					}else {
						$(".navBar").removeClass("searchNavBar");
						$(".mainSearch").show();
						$(".searchResult").hide();
					}
				})
				//清空输入内容
				$(".removeIcon").click(function(){
					$(".search").val('');
					$(".search").focus();
				})
				// 清空历史搜索
				$(".dustbin").click(function(){
					$(".searchHistory").hide();
				})
				// tab切换点击效果
				$(".resultTitle li").click(function(){
					$(".tab").hide().eq($(this).index()).show();
					$(".resultTitle li").removeClass("active");
					$(this).addClass("active");
				})
			})
		</script>
